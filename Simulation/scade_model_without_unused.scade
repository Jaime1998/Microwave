microhondas(
      #pragma ed    <Variable oid="!ed/de28/6D32/534/5e3f26c15a2c"/>
#end

time : int32; 
      #pragma ed    <Variable oid="!ed/dede/6D32/534/5e3f300354d3"/>
#end

