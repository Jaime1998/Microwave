      #pragma ed    <Variable oid="!ed/df40/6D32/534/5e3f31e04b8"/>
#end

started : bool)
returns (
      #pragma ed    <Variable oid="!ed/de31/6D32/534/5e3f26ec4a90"/>
#end

remainingTime : int32; 
      #pragma ed    <Variable oid="!ed/dfce/6D32/534/5e3f33164128"/>
#end

finished : bool default = false last = false)
let
   #pragma ed    <StateMachine oid="!ed/de1c/6D32/534/5e3f25ca2537"/>
#end

   automaton Microondas
      initial       #pragma ed       <State oid="!ed/de21/6D32/534/5e3f264a57de"/>
#end

state Setup
      unless
         #pragma ed          <Transition oid="!ed/df42/6D32/534/5e3f31e43968"/>
#end

         if started
            restart Running;

            #pragma ed       <State oid="!ed/de6b/6D32/534/5e3f285e7d37"/>
#end

state Running
      unless
         #pragma ed          <Transition oid="!ed/e03c/6D32/534/5e3f350c3304"/>
#end

         if opened
            restart Stopped;
         #pragma ed          <Transition oid="!ed/dfbb/6D32/534/5e3f32f14cb0"/>
#end

         if finished
            restart Setup;

      let
         #pragma ed          <StateMachine oid="!ed/de9a/6D32/534/5e3f2f5ad06"/>
#end

         automaton making
            initial             #pragma ed             <State oid="!ed/de9f/6D32/534/5e3f2f5fccc"/>
#end

state Cooking
            var
                              #pragma ed                <Variable oid="!ed/defc/6D32/534/5e3f3040fcb"/>
#end

_L1 : int32;
                              #pragma ed                <Variable oid="!ed/df05/6D32/534/5e3f30491010"/>
#end

_L2 : int32;
                              #pragma ed                <Variable oid="!ed/df22/6D32/534/5e3f30951765"/>
#end

_L3 : int32;
                              #pragma ed                <Variable oid="!ed/df5b/6D32/534/5e3f32374995"/>
#end

_L4 : int32;
                              #pragma ed                <Variable oid="!ed/df65/6D32/534/5e3f3248797b"/>
#end

_L5 : int32;
                              #pragma ed                <Variable oid="!ed/dfdd/6D32/534/5e3f33d76fd3"/>
#end

_L7 : bool;
                              #pragma ed                <Variable oid="!ed/dfe7/6D32/534/5e3f33e1a69"/>
#end

_L8 : int32;
            let
               #pragma ed                <Equation oid="!ed/defb/6D32/534/5e3f30407aa"/>
#end

               _L1 = _L3 #1  - _L2;
               #pragma ed                <Equation oid="!ed/df04/6D32/534/5e3f304977f6"/>
#end

               _L2 = 1;
               #pragma ed                <Equation oid="!ed/df21/6D32/534/5e3f30954b1b"/>
#end

               _L3 =  #1 pre _L5;
               #pragma ed                <Equation oid="!ed/df5a/6D32/534/5e3f323726e9"/>
#end

               _L4 = time;
               #pragma ed                <Equation oid="!ed/df64/6D32/534/5e3f32481412"/>
#end

               _L5 = (_L4) #1  -> (_L1);
               #pragma ed                <Equation oid="!ed/dfaa/6D32/534/5e3f32c137b"/>
#end

               remainingTime = _L5;
               #pragma ed                <Equation oid="!ed/dfdc/6D32/534/5e3f33d75192"/>
#end

               _L7 = _L5 #1  = _L8;
               #pragma ed                <Equation oid="!ed/dfe6/6D32/534/5e3f33e16d97"/>
#end

               _L8 = 0;
               #pragma ed                <Equation oid="!ed/dff3/6D32/534/5e3f33f63651"/>
#end

               finished = _L7;
            tel
         returns .. ;
      tel
            #pragma ed       <State oid="!ed/e02f/6D32/534/5e3f34e615a0"/>
#end

state Stopped
      unless
         #pragma ed          <Transition oid="!ed/e047/6D32/534/5e3f3518406"/>
#end

         if  #1  not opened
            restart Running;

   returns .. ;
tel

